{
    "expo": {
        "name": "ChatApp",
        "slug": "ChatApp",
        "privacy": "public",
        "sdkVersion": "32.0.0",
        "androidStatusBar": {
            "barStyle": "light-content",
            "backgroundColor": "#222265"
        },
        "platforms": ["ios", "android"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#222265"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.rajatkn.chatapp",
            "googleServicesFile": "./google-services.json"
        }
    }
}