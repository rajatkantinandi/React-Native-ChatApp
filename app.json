{
  "expo": {
    "name": "ChatApp",
    "slug": "ChatApp",
    "privacy": "public",
    "description": "ChatApp built using React Native with @pusher/ChatKit API",
    "sdkVersion": "32.0.0",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#222265"
    },
    "platforms": ["android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#222265"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.rajatkn.chatapp"
    }
  }
}
